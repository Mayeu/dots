[color]
   ui = true

[color "branch"]
   current = yellow reverse
   local = yellow
   remote = green

[color "diff"]
   meta = yellow
   frag = magenta
   old = red
   new = green
   whitespace = red reverse

[color "status"]
   added = yellow
   changed = green
   untracked = cyan

[user]
   name = Mayeu
   email = m@6x9.fr

[alias]
   s = status
   c = commit
   ci = commit -a
   br = branch
   co = checkout
   a = add
   d = diff
   vimdiff = difftool -y -t vimdiff

[push]
   default = current

[core]
   excludesfile = ~/.cvsignore
